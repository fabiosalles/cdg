<Schema name='BrazilPopulation'>

    <Cube name="BrazilPopulation">
    <!-- The fact table is always necessary. -->
    <Table name="myfact"/>
    <Dimension name="Province">
    <Hierarchy hasAll="true">
    <!-- No table element here. Fact table is assumed. -->
    <Level name="Province" column="regionAbbrev" ordinalColumn='regionAbbrev' nameColumn='regionFull' uniqueMembers="true" />
    </Hierarchy>
    </Dimension>
    <!-- other dimensions and measures -->
   <Dimension name="Country">
    <Hierarchy hasAll="true">
    <!-- No table element here. Fact table is assumed. -->
    <Level name="Country" column="country" uniqueMembers="true" />
    </Hierarchy>
    </Dimension>        
   <Dimension name="Gender">
    <Hierarchy hasAll="true">
    <!-- No table element here. Fact table is assumed. -->
    <Level name="Gender" column="gender" uniqueMembers="true" />
    </Hierarchy>
    </Dimension>    
   <Dimension name="Time">
    <Hierarchy hasAll="true">
    <!-- No table element here. Fact table is assumed. -->
    <Level name="Year" column="year" type='Numeric' uniqueMembers="true" />
    <Level name="Month" column="monthNum"  ordinalColumn='monthNum' nameColumn='monthFull' type='Numeric' uniqueMembers="true" />    
    </Hierarchy>
    </Dimension>    
    <Measure name="Population" column="value" aggregator="sum" datatype="Integer" formatString="#,###"/>

    
    </Cube>
</Schema>